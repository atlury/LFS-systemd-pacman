# Maintainer: archetech trw2006@hotmail.com
pkgname=file
pkgver=5.29
pkgrel=1
pkgdesc="File package contains a utility for identifying types of files"
arch=('x86_64')
url="http://www.darwinsys.com/file/"
license=('custom')
depends=('zlib')
provides=('file')
conflicts=('file')
options=()
source=("ftp://ftp.astron.com/pub/file/$pkgname-$pkgver.tar.gz")
md5sums=('b7b57b36b577c9cede2f2d1342203302')


build() {
	cd "$pkgname-$pkgver"
	./configure --prefix=/usr
	make
}

check() {
	cd "$pkgname-$pkgver"
	make  check
}

package() {
	cd "$pkgname-$pkgver"
	make DESTDIR="${pkgdir}" install
}
